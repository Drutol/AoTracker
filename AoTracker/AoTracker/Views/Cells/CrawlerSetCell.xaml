<?xml version="1.0" encoding="UTF-8"?>

<controls:CustomViewCell xmlns="http://xamarin.com/schemas/2014/forms"
                         xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                         xmlns:d="http://xamarin.com/schemas/2014/forms/design"
                         xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                         xmlns:util="clr-namespace:AoTracker.Util;assembly=AoTracker"
                         xmlns:controls="clr-namespace:AoTracker.Controls;assembly=AoTracker"
                         mc:Ignorable="d" SelectedBackgroundColor="{DynamicResource SelectedListViewItemColour}"
                         x:Class="AoTracker.Views.Cells.CrawlerSetCell">

    <d:ViewCell.BindingContext>
        <x:Static Member="util:DesignTimeBindingContextProvider.CrawlerSet" />
    </d:ViewCell.BindingContext>

    <ViewCell.View>
        <StackLayout>
            <Label Text="{Binding Name}" />
        </StackLayout>
    </ViewCell.View>
</controls:CustomViewCell>